    "@goat-sdk/core": "^0.5.0",
    "@goat-sdk/crossmint": "^0.5.1",
    "@goat-sdk/wallet-viem": "^0.3.0",
    "@langchain/anthropic": "^0.3.24",
    "@langchain/core": "^0.3.64",
    "@langchain/langgraph": "^0.3.9",
    "@langchain/openai": "^0.6.1",
    "@types/node-fetch": "^2.6.12",
    "@xmtp/content-type-wallet-send-calls": "^2.0.0",
    "@xmtp/node-sdk": "^3.2.0",
    "ai": "^4.0.3",
    "langchain": "^0.3.30",
    "node-fetch": "^3.3.2",
    "serpapi": "2.1.0",
    "uint8arrays": "^5.1.0",
    "viem": "^2.23.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8.0.0"
  },

  "keywords": [
    "crossmint",
    "cookbook",
    "recipes",
